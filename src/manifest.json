{
	"manifest_version": 2,
	"name": "New Tab Page",
	"description": "__MSG_description__",
	"minimum_chrome_version": "63",
	"version": "1.0.0",
	"icons": {
		"16": "icons/icon_16.png",
		"48": "icons/icon_48.png",
		"128": "icons/icon_128.png"
	},
	"default_locale": "en",
	"chrome_url_overrides" : {
		"newtab": "new_tab.html"
	},
	"permissions": [
		"chrome://favicon/",
		"storage",
		"topSites"
	],
	"content_security_policy": "object-src 'self'; script-src 'self'; img-src 'self' data: chrome://favicon/;"
}
