{
	"manifest_version": 3,
	"name": "New Tab Page",
	"description": "__MSG_description__",
	"minimum_chrome_version": "88",
	"version": "1.1.0",
	"icons": {
		"16": "icons/icon_16.png",
		"48": "icons/icon_48.png",
		"128": "icons/icon_128.png"
	},
	"default_locale": "en",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": ["storage", "topSites", "cookies", "favicon"],
	"web_accessible_resources": [
		{
			"resources": ["_favicon/*"],
			"matches": ["<all_urls>"],
			"extension_ids": ["*"]
		}
	]
}
